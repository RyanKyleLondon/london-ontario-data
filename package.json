{
  "name": "london-ontario-data",
  "version": "0.0.1",
  "description": "London Ontario data",
  "main": "server.js",
  "scripts": {
    "clean": "rimraf build/",
    "start": "nodemon server.js",
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register --recursive --require ignore-styles --require ./test/test_helper.js",
    "test:watch": "npm run test -- --watch",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack -p --config ./webpack.production.config.js --progress --color",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "precommit": "npm run lint && npm run test",
    "commit": "git-cz",
    "lint": "eslint app/",
    "csvtojson": "babel-node app/utils/converter.js",
    "scrap": "babel-node app/utils/scrapStops.js",
    "scrap:All": "babel-node app/utils/iterateScrap.js",
    "combineStops": "babel-node app/utils/bundleStopsFolder.js",
    "x": "babel-node x.js",
    "stopIDMap": "babel-node app/utils/convertStopsfile.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/natac13/london-ontario-data.git"
  },
  "keywords": [
    "React",
    "Redux",
    "D3"
  ],
  "author": "Sean Campbell <seancampbell13@gmail.com> (http://seancampbellnatac.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/natac13/london-ontario-data/issues"
  },
  "homepage": "https://github.com/natac13/london-ontario-data#readme",
  "dependencies": {
    "axios": "0.9.1",
    "babel": "6.3.13",
    "babel-cli": "6.3.17",
    "babel-core": "6.3.21",
    "babel-loader": "6.2.0",
    "babel-polyfill": "6.3.14",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "bluebird": "3.3.3",
    "classnames": "2.2.1",
    "css-loader": "0.23.1",
    "dotenv": "2.0.0",
    "express": "4.13.3",
    "extract-text-webpack-plugin": "0.9.1",
    "file-loader": "0.8.5",
    "history": "1.17.0",
    "html-loader": "0.4.0",
    "html-webpack-plugin": "1.7.0",
    "image-webpack-loader": "1.6.2",
    "immutable": "3.7.6",
    "isomorphic-fetch": "2.2.1",
    "json-loader": "0.5.4",
    "modularscale-sass": "2.1.1",
    "mongodb": "2.1.7",
    "mongoose": "4.4.5",
    "node-sass": "3.4.2",
    "normalize.css": "3.0.3",
    "path": "0.12.7",
    "ramda": "0.18.0",
    "react": "0.14.7",
    "react-addons-css-transition-group": "0.14.7",
    "react-dom": "0.14.7",
    "react-fa": "4.0.0",
    "react-immutable-proptypes": "1.5.0",
    "react-pure-render": "1.0.2",
    "react-redux": "4.0.4",
    "react-router": "1.0.3",
    "react-tap-event-plugin": "0.2.2",
    "react-toolbox": "0.14.1",
    "redux": "3.2.1",
    "redux-actions": "0.9.0",
    "redux-async-state-reducer": "0.1.2",
    "redux-form": "4.1.6",
    "redux-immutable": "3.0.3",
    "redux-localstorage": "0.4.0",
    "redux-logger": "2.3.1",
    "redux-promise": "0.5.1",
    "redux-simple-router": "2.0.3",
    "sass-loader": "3.1.2",
    "style-loader": "0.13.0",
    "webpack": "1.12.9"
  },
  "devDependencies": {
    "autoprefixer": "6.2.1",
    "babel-eslint": "4.1.7",
    "babel-plugin-react-transform": "2.0.0-beta1",
    "babel-preset-react-hmre": "1.0.0",
    "chai": "3.4.1",
    "chai-as-promised": "5.2.0",
    "chai-immutable": "1.5.3",
    "commitizen": "2.4.6",
    "cross-env": "1.0.6",
    "csvtojson": "0.5.1",
    "cz-conventional-changelog": "1.1.5",
    "enzyme": "2.0.0",
    "eslint": "2.2.0",
    "eslint-config-standard": "5.1.0",
    "eslint-config-standard-jsx": "1.1.1",
    "eslint-config-standard-react": "2.3.0",
    "eslint-plugin-babel": "3.0.0",
    "eslint-plugin-promise": "1.0.8",
    "eslint-plugin-react": "4.1.0",
    "eslint-plugin-standard": "1.3.2",
    "flux-standard-action": "0.6.0",
    "gh-pages": "0.8.0",
    "ghooks": "1.0.3",
    "ignore-styles": "1.2.0",
    "immutable-devtools": "0.0.5",
    "jsdom": "7.2.1",
    "mocha": "2.3.4",
    "node-loader": "0.5.0",
    "nodemon": "1.9.1",
    "npm-install-webpack-plugin": "2.0.2",
    "postcss-loader": "0.8.0",
    "react-addons-test-utils": "0.14.7",
    "react-transform-catch-errors": "1.0.0",
    "react-transform-hmr": "1.0.1",
    "redbox-react": "1.2.0",
    "rimraf": "2.5.0",
    "toolbox-loader": "0.0.3",
    "url-loader": "0.5.7",
    "validate-commit-msg": "1.1.1",
    "webpack-dev-middleware": "1.4.0",
    "webpack-hot-middleware": "2.6.0",
    "x-ray": "git@github.com:lapwinglabs/x-ray.git#bugfix/nested-crawling"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert",
        "custom"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100
    },
    "ghooks": {
      "pre-commit": "npm run lint && npm run test",
      "commit-msg": "validate-commit-msg"
    }
  },
  "standard": {
    "globals": [
      "describe",
      "context",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it",
      "expect"
    ]
  }
}
